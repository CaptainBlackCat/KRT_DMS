<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Sub_Reception_Doc_Red_SmallIcon_Style">
        <item name="android:contentDescription">@string/base_contentDescription</item>
        <item name="android:layout_height">@dimen/base_5</item>
        <item name="android:layout_width">@dimen/base_5</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
    </style>

    <style name="Sub_Reception_Doc_Icon_Style">
        <item name="android:contentDescription">@string/base_contentDescription</item>
        <item name="android:layout_height">@dimen/base_18</item>
        <item name="android:layout_width">@dimen/base_18</item>
        <item name="android:layout_marginEnd">@dimen/base_10</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintRight_toRightOf">parent</item>
    </style>

    <style name="Sub_Reception_Doc_TextView_Style">
        <item name="android:layout_marginTop">@dimen/base_10</item>
        <item name="android:layout_marginBottom">@dimen/base_10</item>
        <item name="android:layout_marginStart">@dimen/base_15</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">@dimen/base_font_size_1</item>
        <item name="android:textColor">@color/base_black</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="layout_constraintLeft_toLeftOf">parent</item>
        <item name="tvMaxFontNum">4</item>
    </style>

    <style name="Sub_Reception_Doc_EditView_Style">
        <item name="android:layout_marginStart">@dimen/base_15</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:maxLines">1</item>
        <item name="android:layout_marginTop">@dimen/base_10</item>
        <item name="android:layout_marginBottom">@dimen/base_10</item>
        <item name="android:background">@null</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="android:textSize">@dimen/base_font_size_1</item>
        <item name="android:textColor">@color/base_black</item>
    </style>

    <style name="Sub_Reception_Doc_Radio_Group">
        <item name="android:gravity">center</item>
        <item name="android:layout_marginStart">@dimen/base_15</item>
        <item name="layout_constraintTop_toTopOf">parent</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="Sub_Reception_Doc_Open_More">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">@dimen/base_font_size</item>
        <item name="android:textColor">@color/base_black</item>
        <item name="android:paddingTop">@dimen/base_10</item>
        <item name="android:paddingBottom">@dimen/base_10</item>
        <item name="android:gravity">center</item>
        <item name="android:background">#f5f6f1</item>
    </style>

</resources>